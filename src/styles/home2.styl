.main
  min-height: 100vh
  .bg
    background-image: url(../../assets/new/bgbg.png)
    background-size: 100% auto
    background-position: center bottom
    height: 100vh
    background-repeat: no-repeat
    position: fixed
    width: 100%
    z-index: -1
  .bg-c
    background-color: #f8f8f8
    height: 100vh
    width: 100%
    position: fixed
    z-index: -2
.my-swipe
  width: 100%
  height 210px
  .swipe-item 
    width: 100%
    height: 100%
    .img 
      display: block
      width: 100%
      height: 100%

.c-name
  font-size: 15px
  line-height: 48px
  color: #0095ee
  text-align: center

.block
  padding-left: 25px
  padding-right: 25px
  width: 100%
  box-sizing: border-box
  display: flex
  justify-content: center
  align-items: center
  margin-top: 30px
  .img
    display: block
    width: 100%
    height: 90px

.gird-x
  width: 100%
  display: flex
  justify-content: space-between
  align-items: center
  flex-wrap: wrap
  box-sizing: border-box
  // padding-top: 5px
  padding-left: 25px
  padding-right: 25px
  .item
    width: 142px
    height: 90px
    margin-top: 30px
    &:first-child, &:nth-child(2)
      margin-top: 5px
    .img 
      display: block
      width: 100%
      height: 100%      

.mobile-contract
  width: 100%
  box-sizing: border-box
  padding: 0 20px
  margin-top: 30px
  .img 
    display: block
    width: 100%

.myPop-over
  opacity 0

.wenan
  width: calc(100vw - 30px)
  margin: 0 auto
  background: transparent
  .img 
    display: block
    background: transparent
    width: 100%

.logo
  margin-top: 30px
  display: flex
  justify-content: center
  align-items: center
  margin-bottom: 20px
  .img 
    display: block
    height: 50px